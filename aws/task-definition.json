  
{
    "containerDefinitions": [
      {
        "image": "033686261524.dkr.ecr.us-east-1.amazonaws.com/%REPO%",
        "essential": true,
        "name": "%CONTAINERNAME%",
        "cpu": 256,
        "memory": 512,
        "portMappings": [
          {
            "hostPort": 80,
            "protocol": "tcp",
            "containerPort": %CONTAINERPORT%
